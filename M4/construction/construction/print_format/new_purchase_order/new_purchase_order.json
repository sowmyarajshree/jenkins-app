 "html": "<!DOCTYPE html>\n<div id=\"header-html\">\n        {% if letter_head %}\n    <div class=\"letter-head\">\n        {{ letter_head }}\n    </div>\n        {% endif %}\n</div>\n<!---->\n<br>\n\n <div class=\"print-heading\">\n\t\t<h2>Purchase Order<br>\n\t        <small>{{doc.name}}</small>\n        </h2>\n    </div>\n<table  width=\"100%\">\n<thead>\n  <tr>\n    <td><b>Supplier</b></td>\n    <td>{{doc.supplier}}</td>\n    <td><b>Date</b></td>\n    <td>{{doc.transaction_date}}</td>\n  </tr>\n</thead>\n<tbody>\n  <tr>\n    <td><b>Project Name</b></td>\n    <td>{{doc.project_name or \"\"}}</td>\n    \n    <td><b>Required By</b></td>\n    <td>{{doc.schedule_date}}</td>\n  </tr>\n  <tr>\n    <td><b>Shipping Address</b></td>\n    <td>{{doc.shipping_address_display}}</td>\n   \n  </tr>\n</tbody>\n</table>\n<!---->\n<br>\n\n<table  border = 1 width=\"100%\">\n<thead>\n  <tr>\n    <th style=\"width:10px;\">S.No</th>\n    <th><b>Description</b></th>\n    <th><b>Quantity</b></th>\n    <th><b>UOM</b></th>\n    <th><b>Rate</b></th>\n    <th><b>Amount</b></th>\n  </tr>\n</thead>\n<tbody>\n    {% for i in doc.items %}\n  <tr>\n    <td>{{i.idx}}</td>\n    <td>{{i.item_name}} - {{i.description}}</td>\n    <td>{{i.qty or \"\"}}</td>\n    <td>{{i.uom}}</td>\n    <td>{{i.rate}}</td>\n    <td>{{i.amount}}</td>\n  </tr>\n    {% endfor %}\n</tbody>\n</table>\n<br>\n<!---->\n\n<div>\n    <table width = \"100%\">\n<thead>\n  <tr>\n    <td >Total Quantity</td>\n    <td >{{doc.total_qty}}</td>\n    <td align=\"right\">Total</td>\n    <td align=\"right\">{{doc.total}}</td>\n  </tr>\n</thead>\n<tbody>\n  <tr>\n    <td >Tax Category</td>\n    <td>{{doc.tax_category}}</td>\n   \n  </tr>\n</tbody>\n</table>\n</div>\n\n<br>\n<!---->\n<div>\n    {{doc.other_charges_calculation or \"\"}}\n</div>\n<br>\n\n<table>\n                <tr>\n                    <td class=\"col-xs-3\" align =\"right\"><b>Grand Total</b></td>\n                    <td class=\"col-xs-2\" align =\"right\">{{doc.get_formatted(\"grand_total\")}}</td>\n                </tr>\n                <tr>\n             \n                    <td class=\"col-xs-3\" align =\"right\"><b>Rounded Total</b></td>\n                    <td class=\"col-xs-2\" align =\"right\">\u20b9{{\"%0.2f\"% doc.rounded_total | float }}</td>\n                </tr>\n                \n                \n                \n        </table> \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
